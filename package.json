{
  "name": "git-smooth-checkout",
  "displayName": "Git Smooth Checkout",
  "description": "Smoothly checkout in current git repository without repository selection, even in projects with submodules.",
  "version": "0.0.2",
  "publisher": "shimizu",
  "repository": "https://github.com/shimizu1995/git-smooth-checkout",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "SCM Providers",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "extensionDependencies": [
    "vscode.git"
  ],
  "contributes": {
    "commands": [
      {
        "command": "git-smooth-checkout.checkout",
        "title": "Checkout to in the current repository",
        "category": "Git"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/node": "^20",
    "@types/vscode": "^1.73.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.11.0"
  }
}
